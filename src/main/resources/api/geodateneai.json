          "type": {
            "type": "string",
            "description": "Definiert den Geometrietyp in der Ausprägung als Multipolygon oder als Point."
          }
        },
        "discriminator": {
          "propertyName": "type"
        }
      },
      "StadtbezirkDto": {
        "type": "object",
        "properties": {
          "objectId": {
            "type": "integer",
            "format": "int64"
          },
          "stadtbezirkNummer": {
            "type": "string"
          },
          "name": {
            "type": "string"
