{
  "openapi": "3.0.1",
  "info": {
    "title": "DAVE GEODATA EAI",
    "description": "DAVE GEODATA EAI - Service zum Abfragen von Geoinformationen",
    "contact": {
      "name": "DAVE Management",
      "email": "unbekannt@muenchen.de"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8088",
      "description": "Generated server url"
    }
  ],
  "tags": [
    {
      "name": "Messwerte",
      "description": "API zum Abfragen des FeatureServers für Messwerte."
    },
    {
      "name": "Messstelle",
      "description": "API zum Abfragen des FeatureServers für die Messstellen."
    }
  ],
  "paths": {
    "/messwerte/intervals": {
      "post": {
        "tags": [
          "Messwerte"
        ],
        "summary": "Extrahiert die Intervalle und führt eine Aggregation, Summierung und Durchschnittsbildung auf die Intervalle durch. Zum einen wird der Durchschnitt für jeden Messtag je Messquerschnitt der Messstelle über alle Tagesintervalle gebildet.Und zum anderen der Durchschnitt für jeden Messtag je Interval über die selektierten Messquerschnitte.",
        "operationId": "getIntervalle",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MesswertRequestDto"
              }
            }
          },
          "required": true
        },
        "responses": {
          "500": {
            "description": "Bei der Erstellung oder Durchführung des Requests ist ein Fehler aufgetreten.",
            "content": {
              "application/hal+json": {
                "schema": {
                  "$ref": "#/components/schemas/InformationResponseDto"
                }
              }
            }
          },
          "200": {
            "description": "Intervalle erfolgreich abgefragt.",
            "content": {
              "application/hal+json": {
                "schema": {
                  "$ref": "#/components/schemas/IntervalResponseDto"
                }
              }
            }
          }
        }
      }
    },
    "/messwerte/daily-aggregates": {
      "post": {
        "tags": [
          "Messwerte"
        ],
        "summary": "Extrahiert die Tagesaggregate und führt eine Durchschnittsbildung auf die Tagesaggregate pro Messquerschnitt durch.",
        "operationId": "getMeanOfDailyAggregatesPerMQ",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TagesaggregatRequestDto"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Tagesaggregate erfolgreich abgefragt.",
            "content": {
              "application/hal+json": {
                "schema": {
                  "$ref": "#/components/schemas/TagesaggregatResponseDto"
                }
              }
            }
          },
          "500": {
            "description": "Bei der Erstellung oder Durchführung des Requests ist ein Fehler aufgetreten.",
            "content": {
              "application/hal+json": {
                "schema": {
                  "$ref": "#/components/schemas/InformationResponseDto"
                }
              }
            }
          }
        }
      }
    },
    "/messstelle": {
      "get": {
        "tags": [
          "Messstelle"
        ],
        "summary": "Holt alle relevanten Messstellen.",
        "operationId": "getMessstellen",
        "responses": {
          "200": {
            "description": "Messstellen erfolgreich abgefragt.",
            "content": {
              "application/hal+json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/MessstelleDto"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Bei der Erstellung oder Durchführung des Requests ist ein Fehler aufgetreten.",
            "content": {
              "application/hal+json": {
                "schema": {
                  "$ref": "#/components/schemas/InformationResponseDto"
                }
              }
            }
          }
        }
      }
    },
    "/messstelle/unauffaellige-tage-for-each-messstelle": {
      "get": {
        "tags": [
          "Messstelle"
        ],
        "summary": "Gibt die unauffälligen Tage für jede existierende Messstelle im gegebenen Zeitraum zurück.",
        "operationId": "getUnauffaelligeTageForEachMessstelle",
        "parameters": [
          {
            "name": "start-date",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "end-date",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date"
            }
          }
        ],
        "responses": {
          "500": {
            "description": "Bei der Erstellung oder Durchführung des Requests ist ein Fehler aufgetreten.",
            "content": {
              "application/hal+json": {
                "schema": {
                  "$ref": "#/components/schemas/InformationResponseDto"
                }
              }
            }
          },
          "200": {
            "description": "Unauffällige Tage erfolgreich abgefragt.",
            "content": {
              "application/hal+json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/UnauffaelligerTagDto"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LocalTime": {
        "type": "object",
        "properties": {
          "hour": {
            "type": "integer",
            "format": "int32"
          },
          "minute": {
            "type": "integer",
            "format": "int32"
          },
          "second": {
            "type": "integer",
            "format": "int32"
          },
          "nano": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "MesswertRequestDto": {
        "required": [
          "allMessquerschnittIds",
          "endDate",
          "endTime",
          "intervalInMinutes",
          "selectedMessquerschnittIds",
          "startDate",
          "startTime",
          "tagesTyp"
        ],
        "type": "object",
        "properties": {
          "allMessquerschnittIds": {
            "type": "array",
            "description": "Alle Messquerschnitt-Ids der Messstelle",
            "items": {
              "type": "integer",
              "description": "Alle Messquerschnitt-Ids der Messstelle",
              "format": "int32"
            }
          },
          "selectedMessquerschnittIds": {
            "type": "array",
            "description": "Die ausgewählten Messquerschnitt-Ids der Messstelle welche angezeigt werden sollen.",
            "items": {
              "type": "integer",
              "description": "Die ausgewählten Messquerschnitt-Ids der Messstelle welche angezeigt werden sollen.",
              "format": "int32"
            }
          },
          "intervalInMinutes": {
            "type": "string",
            "enum": [
              "INTERVAL_15",
              "INTERVAL_30",
              "INTERVAL_60"
            ]
          },
          "startDate": {
            "type": "string",
            "format": "date"
          },
          "endDate": {
            "type": "string",
            "format": "date"
          },
          "startTime": {
            "$ref": "#/components/schemas/LocalTime"
          },
          "endTime": {
            "$ref": "#/components/schemas/LocalTime"
          },
          "tagesTyp": {
            "type": "string",
            "enum": [
              "DTV_W3",
              "DTV_W5",
              "SAMSTAG",
              "SONNTAG_FEIERTAG",
              "WERKTAG_FERIEN",
              "DTV"
            ]
          }
        }
      },
      "InformationResponseDto": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "ERROR",
              "WARNING",
              "INFORMATION_SUCCESS",
              "INFORMATION_NEUTRAL"
            ]
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "traceId": {
            "type": "string"
          },
          "spanId": {
            "type": "string"
          },
          "httpStatus": {
            "type": "integer",
            "format": "int32"
          },
          "originalException": {
            "type": "string"
          },
          "messages": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "IntervalDto": {
        "type": "object",
        "properties": {
          "mqId": {
            "type": "integer",
            "format": "int32"
          },
          "anzahlLfw": {
            "type": "number"
          },
          "anzahlKrad": {
            "type": "number"
          },
          "anzahlLkw": {
            "type": "number"
          },
          "anzahlBus": {
            "type": "number"
          },
          "anzahlRad": {
            "type": "number"
          },
          "summeAllePkw": {
            "type": "number"
          },
          "summeLastzug": {
            "type": "number"
          },
          "summeGueterverkehr": {
            "type": "number"
          },
          "summeSchwerverkehr": {
            "type": "number"
          },
          "summeKraftfahrzeugverkehr": {
            "type": "number"
          },
          "prozentSchwerverkehr": {
            "type": "number"
          },
          "prozentGueterverkehr": {
            "type": "number"
          },
          "datumUhrzeitVon": {
            "type": "string",
            "format": "date-time"
          },
          "datumUhrzeitBis": {
            "type": "string",
            "format": "date-time"
          },
          "tagesTyp": {
            "type": "string",
            "enum": [
              "DTV_W3",
              "DTV_W5",
              "SAMSTAG",
              "SONNTAG_FEIERTAG",
              "WERKTAG_FERIEN",
              "DTV"
            ]
          }
        },
        "description": "Für jeden Interval über jeden Messtag den Durchschnitt über die Summe selektierten Messquerschnitte."
      },
      "IntervalResponseDto": {
        "type": "object",
        "properties": {
          "meanOfSummedUpDailyIntervalsForEachMessquerschnittOverMesstage": {
            "type": "array",
            "description": "Für jeden Messquerschnitt den Durchschnitt der Messtage über die Tagessumme.",
            "items": {
              "$ref": "#/components/schemas/IntervalsForMqIdDto"
            }
          },
          "meanOfSummedUpMessquerschnitteForEachIntervalOverMesstage": {
            "type": "array",
            "description": "Für jeden Interval über jeden Messtag den Durchschnitt über die Summe selektierten Messquerschnitte.",
            "items": {
              "$ref": "#/components/schemas/IntervalDto"
            }
          },
          "meanForEachIntervalAndEachMessquerschnittOverMesstage": {
            "type": "array",
            "description": "Für jeden Intervall und jede Messquerschnitt-Id den Durchschnitt über die Messtage.",
            "items": {
              "$ref": "#/components/schemas/IntervalsForMqIdDto"
            }
          },
          "includedMeasuringDays": {
            "type": "integer",
            "description": "Anzahl der Messtage, die in die Auswertung eingeflossenen sind.",
            "format": "int32"
          }
        }
      },
      "IntervalsForMqIdDto": {
        "type": "object",
        "properties": {
          "mqId": {
            "type": "integer",
            "format": "int32"
          },
          "intervals": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/IntervalDto"
            }
          }
        },
        "description": "Für jeden Intervall und jede Messquerschnitt-Id den Durchschnitt über die Messtage."
      },
      "TagesaggregatRequestDto": {
        "required": [
          "messquerschnittIds",
          "tagesTyp",
          "zeitraeume"
        ],
