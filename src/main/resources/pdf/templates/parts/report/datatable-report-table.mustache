{{#datentabelleZaehldaten}}
  <figure>
    <table class="datentabelle">
      <thead>
      <tr>
        <th colspan="3" class="datentabelle-header datentabelle-border-right">
          Zeit
        </th>
        {{#showTabsFahrzeugtypen}}
          <th colspan="{{ activeTabsFahrzeugtypen }}" class="datentabelle-header datentabelle-border-right">
            Fahrzeugtypen
          </th>
        {{/showTabsFahrzeugtypen}}
        {{#showTabsFahrzeugklassen}}
          <th colspan="{{ activeTabsFahrzeugklassen }}" class="datentabelle-header datentabelle-border-right">
            Fahrzeugklasse
          </th>
        {{/showTabsFahrzeugklassen}}
        {{#showTabsAnteile}}
          <th colspan="{{ activeTabsAnteile }}"
              class="datentabelle-header datentabelle-border-right">
            Anteil
          </th>
        {{/showTabsAnteile}}
        {{#showPkwEinheiten}}
          <th rowspan="2" class="datentabelle-header datentabelle-border-bottom">Pkw-Einheiten</th>
        {{/showPkwEinheiten}}
      </tr>
      <tr>
        <th class="datentabelle-header datentabelle-border-bottom datentabelle-zeit-{{randomTableId}}">
          von
        </th>
        <th class="datentabelle-header datentabelle-border-bottom datentabelle-zeit-{{randomTableId}}">
          bis
        </th>
        <th class="datentabelle-header datentabelle-border-bottom datentabelle-border-right datentabelle-zeit-{{randomTableId}}">

        </th>
        {{#showPersonenkraftwagen}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-personenkraftwagen-{{randomTableId}}">
            Pkw
          </th>
        {{/showPersonenkraftwagen}}
        {{#showLastkraftwagen}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-lastkraftwagen-{{randomTableId}}">
            Lkw
          </th>
        {{/showLastkraftwagen}}
        {{#showLastzuege}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-lastzuege-{{randomTableId}}">
            Lz
          </th>
        {{/showLastzuege}}
        {{#showBusse}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-busse-{{randomTableId}}">
            Bus
          </th>
        {{/showBusse}}
        {{#showKraftraeder}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-kraftraeder-{{randomTableId}}">
            Krad
          </th>
        {{/showKraftraeder}}
        {{#showRadverkehr}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-radverkehr-{{randomTableId}}">
            Rad
          </th>
        {{/showRadverkehr}}
        {{#showFussverkehr}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-fussverkehr-{{randomTableId}}">
            Fu√ü
          </th>
        {{/showFussverkehr}}
        {{#showKraftfahrzeugverkehr}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-fahrzeugklassen-{{randomTableId}} datentabelle-kraftfahrzeugverkehr-{{randomTableId}}">
            KFZ
          </th>
        {{/showKraftfahrzeugverkehr}}
        {{#showSchwerverkehr}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-fahrzeugklassen-{{randomTableId}} datentabelle-schwerverkehr-{{randomTableId}}">
            SV
          </th>
        {{/showSchwerverkehr}}
        {{#showGueterverkehr}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-fahrzeugklassen-{{randomTableId}} datentabelle-gueterverkehr-{{randomTableId}}">
            GV
          </th>
        {{/showGueterverkehr}}
        {{#showSchwerverkehrsanteilProzent}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-anteile-{{randomTableId}} datentabelle-schwerverkehrsanteil-prozent-{{randomTableId}}">
            SV%
          </th>
        {{/showSchwerverkehrsanteilProzent}}
        {{#showGueterverkehrsanteilProzent}}
          <th class="datentabelle-header datentabelle-border-bottom datentabelle-anteile-{{randomTableId}} datentabelle-gueterverkehrsanteil-prozent-{{randomTableId}}">
            GV%
          </th>
        {{/showGueterverkehrsanteilProzent}}
      </tr>
      </thead>
      {{#zaehldatenList}}
        <tr class="datentabelle-row{{type}}">
          <td class="datentabelle-zeit-{{randomTableId}}">
            {{startUhrzeit}}
          </td>
          <td class="datentabelle-zeit-{{randomTableId}}">
            {{endeUhrzeit}}
          </td>
          <td class="datentabelle-border-right datentabelle-zeit-{{randomTableId}}">
            {{type}}
          </td>
          {{#showPersonenkraftwagen}}
            <td class="datentabelle-cell-numeric datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-personenkraftwagen-{{randomTableId}}">
              {{pkw}}
            </td>
          {{/showPersonenkraftwagen}}
          {{#showLastkraftwagen}}
            <td class="datentabelle-cell-numeric datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-lastkraftwagen-{{randomTableId}}">
              {{lkw}}
            </td>
          {{/showLastkraftwagen}}
          {{#showLastzuege}}
            <td class="datentabelle-cell-numeric datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-lastzuege-{{randomTableId}}">
              {{lastzuege}}
            </td>
          {{/showLastzuege}}
          {{#showBusse}}
            <td class="datentabelle-cell-numeric datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-busse-{{randomTableId}}">
              {{busse}}
            </td>
          {{/showBusse}}
          {{#showKraftraeder}}
            <td class="datentabelle-cell-numeric datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-kraftraeder-{{randomTableId}}">
              {{kraftraeder}}
            </td>
          {{/showKraftraeder}}
          {{#showRadverkehr}}
            <td class="datentabelle-cell-numeric datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-radverkehr-{{randomTableId}}">
              {{fahrradfahrer}}
            </td>
          {{/showRadverkehr}}
          {{#showFussverkehr}}
            <td class="datentabelle-cell-numeric datentabelle-fahrzeugtypen-{{randomTableId}} datentabelle-fussverkehr-{{randomTableId}}">
              {{fussgaenger}}
            </td>
          {{/showFussverkehr}}
          {{#showKraftfahrzeugverkehr}}
            <td class="datentabelle-cell-numeric datentabelle-fahrzeugklassen-{{randomTableId}} datentabelle-kraftfahrzeugverkehr-{{randomTableId}}">
              {{kfz}}
            </td>
          {{/showKraftfahrzeugverkehr}}
          {{#showSchwerverkehr}}
            <td class="datentabelle-cell-numeric datentabelle-fahrzeugklassen-{{randomTableId}} datentabelle-schwerverkehr-{{randomTableId}}">
              {{schwerverkehr}}
            </td>
          {{/showSchwerverkehr}}
          {{#showGueterverkehr}}
            <td class="datentabelle-cell-numeric datentabelle-fahrzeugklassen-{{randomTableId}} datentabelle-gueterverkehr-{{randomTableId}}">
              {{gueterverkehr}}
            </td>
          {{/showGueterverkehr}}
          {{#showSchwerverkehrsanteilProzent}}
            <td class="datentabelle-cell-numeric datentabelle-anteile-{{randomTableId}} datentabelle-schwerverkehrsanteil-prozent-{{randomTableId}}">
              {{anteilSchwerverkehrAnKfzProzent}}%
            </td>
          {{/showSchwerverkehrsanteilProzent}}
          {{#showGueterverkehrsanteilProzent}}
            <td class="datentabelle-cell-numeric datentabelle-anteile-{{randomTableId}} datentabelle-gueterverkehrsanteil-prozent-{{randomTableId}}">
              {{anteilGueterverkehrAnKfzProzent}}%
            </td>
          {{/showGueterverkehrsanteilProzent}}
          {{#showPkwEinheiten}}
            <td class="datentabelle-cell-numeric">
              {{pkwEinheiten}}
            </td>
          {{/showPkwEinheiten}}
        </tr>
      {{/zaehldatenList}}
    </table>
    <figcaption class="report">
      {{text}}
    </figcaption>
  </figure>
{{/datentabelleZaehldaten}}